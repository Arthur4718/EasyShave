<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.supermatch.smplay">

    <application
            android:name=".KotlinApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.RegisterActivities.SelectLoginActivity">
        </activity>
        <activity android:name=".activity.RegisterActivities.ConnectionErrorActivity">
        </activity>
        <activity android:name=".activity.RegisterActivities.WellcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.BaseActivities.CarrosActivity">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.supermatch.smplay.activity.RegisterActivities.MainActivity"/>
        </activity>
        <activity
                android:name=".activity.RegisterActivities.MainActivity"
                android:theme="@style/AppTheme.NavDrawer">
        </activity>
    </application>

</manifest>